{
    "name": "sofyco/demo-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle Template",
    "keywords": ["php", "symfony", "bundle", "demo", "template"],
    "homepage": "https://github.com/sofyco/demo-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\DemoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\DemoBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
